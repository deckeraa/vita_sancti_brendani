                <ITEXT CH=" esset vīdit vīsiōnem, id est, sinum suum aurō"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" obrizō plēnum et mamillās suās magnō radiantēs splendōre. Et hanc vīsiōnem nārrāns"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=" Sānctō Ercō Epīscopō, interpretātus est eam dīcēns: Ō mulier beāta, homō magnae fēlīcitātis, plēnus Spīritū Sānctō, ex tē siquidem"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH=" nāscētur. Quod"/>
                <ITEXT FEATURES="inherit superscript" CH="9"/>
                <ITEXT CH=" nōn post multum tempus in Sānctō Brendānō complētum est."/>
                <trail PARENT="Latin"/>
